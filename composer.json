{
  "name": "fidian/phppdb",
  "description": "PHP classes to read/write PalmOS PDB (Palm DataBase) files",
  "minimum-stability": "dev",
  "type": "library",
  "license": "GNU LGPL version 2",
  "authors": [
    {
      "name": "Tyler Akins",
      "email": "fidian@rumkin.com"
    }
  ],
  "config": {
    "vendor-dir": "src/vendor"
  },
  "autoload": {
    "psr-4": {
      "phppdb\\": "src/phppdb/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "phppdb\\tests\\": "src/tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "4.8"
  },
  "scripts": {
    "test": [ "src/vendor/phpunit/phpunit/phpunit --bootstrap src/tests/bootstrap.php src/tests/" ]
  }
}
